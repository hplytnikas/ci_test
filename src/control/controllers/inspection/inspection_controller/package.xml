<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>inspection_controller</name>
  <version>0.0.0</version>
  <description>Controller and executable for the actions needed during inspection of the car during scrutineering</description>
  <maintainer email="digarcia@ethz.ch">Diego Garcia</maintainer>
  <license>BSD</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>abstract_controller</depend>
  <depend>controller_node</depend>
  <depend>autonomous_msgs</depend>
  <depend>vcu_msgs</depend>
  <depend>generate_parameter_library</depend>
  <depend>inspection_visualization_node</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
