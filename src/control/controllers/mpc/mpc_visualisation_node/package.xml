  <version>0.0.0</version>
  <description>Implementation of the visualisation publishers for MPC</description>
  <maintainer email="hakiyama@ethz.ch">Hironobu Akiyama</maintainer>
  <maintainer email="johnemus@ethz.ch">Jonas Ohnemus</maintainer>
  <license>BSD</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>visualization_msgs</depend>
  <depend>geometry_msgs</depend>
